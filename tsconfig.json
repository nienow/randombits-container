{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "isolatedModules": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "esnext",
    "incremental": true,
    "baseUrl": "src",
    "sourceMap": true
  },
  "exclude": [],
  "include": [
    "src"
  ]
}
